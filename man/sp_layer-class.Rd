\alias{"+,ggplot,sp_layer-method"}
\alias{"+,ggsubplot,sp_layer-method"}
\alias{"$,sp_layer-method"}
\alias{"$<-,sp_layer-method"}
\alias{"c,sp_layer-method"}
\alias{"ggtransform,sp_layer-method"}
